# Cloudinary settings for Upload Widget
VITE_CLOUDINARY_CLOUD_NAME=your_cloud_name
# Use an UNSIGNED upload preset created in your Cloudinary console (Settings > Upload > Upload presets)
VITE_CLOUDINARY_UPLOAD_PRESET=your_unsigned_preset
# Optional: default upload folder for the widget
VITE_CLOUDINARY_FOLDER=nadlan/properties
# If you want to use a signed preset like ml_default, set this to true and make sure backend route /api/cloudinary/sign is available
VITE_CLOUDINARY_SIGNED=false
